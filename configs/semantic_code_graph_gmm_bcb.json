{
    "gpu": {
        "use": true,
        "id": 0
    },

    "model": "graph_match_nerual_network",
    "language": "Java",

    "out_dir": "out/semantic_ast_gmn_bcb/",


    "dataset_params":{
        "name": "BigCloneBench",
        "graph_model": "semantic_code_graph",
        "visualization": false,
        "ast_edge": true,
        "value_edge": true,
        "cfg_edge": true,
        "dfg_edge": true,
        "if_augument": true,
        "loops_augument": true
    },

    "params": {
        "seed": 41,
        "epochs": 7,
        "batch_size": 32,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 15,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 2,
        "save_epoch_interval": 2,
        "eval_epoch_interval": 2,
        "threshold": 0
    },

    "net_params": {
        "embedding_dim": 100,
        "edgelen": 20,
        "num_layers": 4
    }
}